<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="579.0" prefWidth="875.0" stylesheets="@../css/library.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminUI.ExitController">
   <children>
      <VBox layoutX="76.0" layoutY="15.0" prefHeight="360.0" prefWidth="322.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="36.0" prefWidth="322.0">
               <children>
                  <Label text="姓名：" />
                  <TextField fx:id="name" editable="false" styleClass="text-area" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="36.0" prefWidth="322.0">
               <children>
                  <Label text="ID：" />
