               <children>
                  <Label text="权限：" />
                  <TextField fx:id="limit" editable="false" styleClass="text-area" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="36.0" prefWidth="322.0">
               <children>
                  <Label text="电话：" />
                  <TextField fx:id="tel" editable="false" styleClass="text-area" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="36.0" prefWidth="322.0">
               <children>
                  <Label text="邮箱：" />
                  <TextField fx:id="mail" editable="false" styleClass="text-area" />
               </children>
            </HBox>

         </children>
      </VBox>
      <Button fx:id="logout" layoutX="664.0" layoutY="529.0" mnemonicParsing="false" onAction="#onLogoutClick" styleClass="warm-theme" text="退出登录" />
      <Button fx:id="exitSystem" layoutX="757.0" layoutY="529.0" mnemonicParsing="false" onAction="#onExitClick" styleClass="warning-theme" text="退出系统" />
   </children>
