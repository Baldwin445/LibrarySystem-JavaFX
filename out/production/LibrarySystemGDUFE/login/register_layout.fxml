                     <String fx:value="男" />
                     <String fx:value="女" />
                  </FXCollections>
               </items>
            </ChoiceBox>
            <TextField fx:id="major" layoutX="375.0" layoutY="160.0" prefHeight="37.0" prefWidth="248.0" promptText="输入专业" />
            <ImageView fx:id="major_tip" fitHeight="15.0" fitWidth="15.0" layoutX="631.0" layoutY="171.0" pickOnBounds="true" preserveRatio="true">
