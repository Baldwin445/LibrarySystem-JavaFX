            <TextField fx:id="name" layoutX="153.0" layoutY="20.0" prefHeight="37.0" prefWidth="248.0" promptText="真实姓名" />
            <TextField fx:id="acct" layoutX="153.0" layoutY="90.0" prefHeight="37.0" prefWidth="248.0" promptText="设置账号" />
