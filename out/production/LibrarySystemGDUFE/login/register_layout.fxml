      <ImageView fx:id="nextstep" fitHeight="37.0" fitWidth="248.0" layoutX="337.0" layoutY="496.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/reg/nextstep.png" />
         </image>
      </ImageView>
      <ImageView fx:id="close" fitHeight="20.0" fitWidth="20.0" layoutX="866.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/close.png" />
         </image>
      </ImageView>
      <ImageView fx:id="major_tip_context" fitHeight="150.0" fitWidth="200.0" layoutX="742.0" layoutY="325.0" pickOnBounds="true" preserveRatio="true">
